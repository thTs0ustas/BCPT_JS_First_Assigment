{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:prettier/recommended","eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "arrow-body-style": "off",
        "prefer-arrow-callback": "off"
    }
}
